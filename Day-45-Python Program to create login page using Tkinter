from tkinter import *
root = Tk()
root.title("Kratika Jadon")
root.geometry('350x200')
label = Label(root, text = "Login Page",fg ='black')
entry1 = Entry(root)
entry2 = Entry(root)
l1 = Label(root, text = "Username")
entry1.insert(0, "Type your username")
l2 = Label(root, text = "Password")
entry2.insert(0, "Type your password")
button = Button(root,text = 'Login', fg = 'white', bg = 'orange')
checkbutton = Checkbutton(root, text = "Remember me")
canvas = Canvas(root, bg = "black", width= 100, height=50)
l1.pack()
entry1.pack()
l2.pack()
entry2.pack()
checkbutton.select()
checkbutton.pack()
button.pack()
canvas.pack
root.mainloop()
