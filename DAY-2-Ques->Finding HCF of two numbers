number1 = int(input("Enter first number: "))  
number2 = int(input("Enter second number: "))  
if number1 > number2:
    minimum = number2
else:
    minimum = number1
for i in range(1, minimum+1):
    if((number1 % i == 0) and (number2 % i == 0)):
        hcf = i 
print("hcf of",number1,"and",number2,"=",hcf)
